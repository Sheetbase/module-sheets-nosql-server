{
   "name": "@sheetbase/sheets-nosql-server",
   "version": "0.0.8",
   "description": "Using Google Sheets as NoSQL database.",
   "author": "Sheetbase",
   "homepage": "https://sheetbase.net",
   "license": "MIT",
   "main": "dist/bundles/sheetbase-sheets-nosql-server.umd.js",
   "module": "dist/fesm3/sheetbase-sheets-nosql-server.js",
   "typings": "dist/sheetbase-sheets-nosql-server.d.ts",
   "files": [
      "dist"
   ],
   "scripts": {
      "module": "sheetbase-app-scripts build",
      "readme": "sheetbase-app-scripts readme",
      "docs": "sheetbase-app-scripts docs",
      "build": "npm run module && npm run readme && npm run docs && npm run transpile-test",
      "deploy": "sheetbase-app-scripts deploy",
      "lint": "tslint --project tslint.json && echo 'No lint errors.'",
      "transpile-test": "tsc --project tests/tsconfig.test.json && replace-in-file /lodash-es/g lodash tests/out-tsc/src/lib/sheets-nosql.js --isRegex",
      "test": "nyc --cache false mocha --timeout 100000 -- tests/out-tsc/tests/*.js",
      "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
   },
   "keywords": [
      "google-apps-script",
      "sheets",
      "sheetbase",
      "database",
      "nosql"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/sheetbase/sheets-nosql-server.git"
   },
   "bugs": {
      "url": "https://github.com/sheetbase/sheets-nosql-server/issues"
   },
   "dependencies": {
      "@sheetbase/core-server": "0.0.13",
      "@sheetbase/tamotsux-server": "0.0.4",
      "lodash-es": "^4.17.11"
   },
   "devDependencies": {
      "@sheetbase/app-scripts": "0.0.28",
      "@types/chai": "^4.1.7",
      "@types/google-apps-script": "0.0.31",
      "@types/lodash-es": "^4.17.1",
      "@types/mocha": "^5.2.5",
      "@types/node": "^10.12.6",
      "chai": "^4.2.0",
      "coveralls": "^3.0.2",
      "mocha": "^5.2.0",
      "nyc": "^13.1.0",
      "replace-in-file": "^3.4.2",
      "rollup-plugin-node-resolve": "^3.4.0",
      "tslint": "^5.11.0",
      "typescript": "^3.1.6"
   }
}
